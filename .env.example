# Copy this file to .env and fill in your actual values

# Google Custom Search API
GOOGLE_API_KEY=your_google_api_key_here
SEARCH_ENGINE_ID=your_search_engine_id_here

# Google Gemini AI API Keys (comma-separated for rotation)
GEMINI_API_KEYS=your_gemini_key_1,your_gemini_key_2,your_gemini_key_3

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Session Cookies (optional)
AMAZON_COOKIE=your_session_cookies_here

# File Paths
EXCEL_FILE=data1.xlsx
LOG_FILE=logfile.txt